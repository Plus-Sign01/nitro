<h1>let's make Project!</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@project) do |f| %>

    <%= f.label :project_name　%>
    <%= f.text_field :project_name %>

    <%= f.label :project_place %>
    <%= f.text_field :project_place %>

    <%= f.label :project_period %>
    <%= f.text_field :project_period　%>

    <%= f.label :content %>
    <%= f.text_field :content %>

    <%= f.label :project_owner %>
    <%= f.text_field :project_owner %>

    <%= f.submit "make project", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
